{
  "name": "node-api",
  "version": "1.0.0",
  "description": "node-api",
  "private": true,
  "egg": {
    "declarations": true
  },
  "dependencies": {
    "egg": "^3",
    "egg-scripts": "^2"
  },
  "devDependencies": {
    "egg-bin": "^5",
    "egg-ci": "^2",
    "egg-mock": "^5",
    "eslint": "^8",
    "eslint-config-egg": "^12"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "start": "egg-scripts start --env=prod --title=node-api --port=3000",
    "stop": "egg-scripts stop --title=node-api --port=3000",
    "dev": "egg-bin dev",
    "debug": "egg-bin debug",
    "test": "yarn run lint -- --fix && yarn run test-local",
    "test-local": "egg-bin test",
    "cov": "egg-bin cov",
    "lint": "eslint .",
    "ci": "yarn run lint && yarn run cov"
  },
  "ci": {
    "version": "16, 18",
    "type": "github"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "allen.wang",
  "license": "MIT"
}
